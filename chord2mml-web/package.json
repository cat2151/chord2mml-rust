{
  "name": "chord2mml-web",
  "version": "0.1.0",
  "description": "Web application for chord2mml-rust",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "npm run build:wasm && npm run build:web",
    "build:wasm": "cd ../chord2mml-wasm && wasm-pack build --target web --out-dir ../chord2mml-web/public/wasm",
    "build:web": "vite build",
    "dev": "vite",
    "preview": "vite preview",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "keywords": ["chord", "mml", "music", "wasm", "rust"],
  "author": "cat2151",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "dependencies": {}
}
